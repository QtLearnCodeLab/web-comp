  "version": 1.1,
  "tags": [
    {
      "name": "qt-board",
      "description": "Board web component - You should use the element as: \n `<qt-board id='board1' board-name='veda1' board-type='VEDA' com-port='COM6'></qt-board>` . ",
      "references": [
        {
          "name": "api-doc",
          "url": "https://qtlearncodelab.github.io/qtpi-shared/qtwc/js/releases/latest/docs/index.html"
        }
      ],
      "attributes": [
        {
          "name": "board-name",
          "description": "Name of the board you are using",
          "values": [{ "name": "veda1" }]
        },
        {
          "name": "board-type",
          "description": "Board Type you are using. Either VEDA or RIO",
          "values": [
            { "name": "VEDA", "description": "VEDA Board" },
            { "name": "RIO", "description": "RIO Board" }
          ]
        }
      ]
    },
    {
      "name": "qt-led",
