        {
          "name": "api-doc",
          "url": "https://qtlearncodelab.github.io/qtpi-shared/qtwc/js/releases/latest/docs/QtLED.html"
        }
      ],
      "attributes": [
        {
          "name": "port",
          "description": "Port number to which LED is connected on board",
          "values": [{ "name": "1" }]
        },
        {
          "name": "board-name",
          "description": "Same as connected board",
          "values": [{ "name": "veda1" }]
        },
        {
          "name": "ccs",
          "description": "Visibility of Control Config Simulation",
          "values": [{ "name": "true" }, { "name": "false" }]
        },
        {
          "name": "state",
          "description": "State of component",
          "values": [{ "name": "on" }, { "name": "off" }]
        }
